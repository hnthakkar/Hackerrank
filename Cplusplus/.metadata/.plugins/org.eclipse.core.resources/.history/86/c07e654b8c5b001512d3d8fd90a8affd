# include <iostream>
#define int arraySize = 50;
using namespace std;

bool arrayEquals(int a[], int b[]){
	bool ret = true;
	for (int i = 0; i < arraySize; i++){
		if(a[i] != b[i]){
			ret = false;
			break;
		}
	}
	return ret;
}

void copyArray(int from[], int to[]){
	for(int i = 0; i < 200; i++){
		to[i] = from[i];
	}
}

void min(int a[], int b[], int c[], int ret[]) {
	int i = 199;
	bool equal = true;
	while (i >= 0) {
		if(a[i] > b[i]){
			copyArray(b, ret);
			equal = false;
			break;
		} if(b[i] > a[i]){
			copyArray(a, ret);
			equal = false;
			break;
		}
		i--;
	}
	if(equal){
		copyArray(b, ret);
	}
	i = 199;
	while (i >= 0) {
		if(ret[i] > c[i]) {
			copyArray(c, ret);
			break;
		}if(c[i] > ret[i]) {
			break;
		}
		i--;
	}
}

void multiply(int nArray[], int num, int result[]){
	int lastIndex = 0, carry = 0;
	for(int j = 0; j < 200; j++){
		int val = nArray[j] * num + carry;
		carry = val/10;
		result[j] = (val%10);
		if(result[j] > 0){
			lastIndex = j;
		}
	}
	while(carry){
		int tmp = carry%10;
		carry = carry/10;
		result[++lastIndex] = tmp;
	}
}


int main() {
	int T;
	cin >> T;

	int p, q, r;
	cin >> p >> q >> r;
	int kCases[T];
	int max = 0;
	for (int i = 0; i < T; i++) {
		cin >> kCases[i];
		if (max < kCases[i]) {
			max = kCases[i];
		}
	}

	long int pIndex = 0, qIndex = 0, rIndex = 0;
	int tmp[200];
	tmp[0] = 1;
	int pNext[200] = {};
	pNext[0] = 1;
	multiply(tmp, p, pNext);
	int qNext[200] = {};
	qNext[0] = 1;
	multiply(tmp, q, qNext);
	int rNext[200] = {};
	rNext[0] = 1;
	multiply(tmp, r, rNext);
	int nextNum[200];
	for(int i = 0; i < 200; i++){
		nextNum[i] = 0;
	}
	int lastNum[200];
	for(int i = 0; i < 200; i++){
		lastNum[i] = 0;
	}
	int num[max][200];
	for (int i = 0; i < max; i++) {
		for (int j = 0; j < 200; j++){
			num[i][j] = 0;
		}
	}
	num[0][0] = 1;

	int i =  1;
	while(i < max) {
		min(pNext, qNext, rNext, nextNum);
		if(!arrayEquals(nextNum,lastNum)){
			copyArray(nextNum, lastNum);
			copyArray(nextNum, num[i]);
			i++;
		}
		if (arrayEquals(nextNum,pNext)) {
			pIndex++;
			multiply(num[pIndex], p, pNext);
		} else if (arrayEquals(nextNum,qNext)) {
			qIndex++;
			multiply(num[qIndex], q, qNext);
		} else if (arrayEquals(nextNum,rNext)) {
			rIndex++;
			multiply(num[rIndex], r, rNext);
		}
	}
	for (int a = 0; a < T; a++) {
		for(int b = 199; b >= 0; b--){
			cout << (num[kCases[a]-1][b]);
		}
		cout << endl;
	}

	return 0;
}
